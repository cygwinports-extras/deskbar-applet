  libgtk2.0_0-2.18.3-1
  libX11_6-1.3.2-1
  python-2.5.4-10
  python-dbus-0.83.0-10
  python-gconf2-2.28.0-1
  python-gnomeapplet-2.28.0-1
  python-gnomedesktop2-2.28.0-1
